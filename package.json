{
  "name": "slack-to-maxqda-adapter-electron",
  "private": true,
  "version": "1.0.2",
  "author": "nae-lab <nae-lab@example.com>",
  "description": "Slack to MAXQDA Adapter - Export Slack conversations for MAXQDA analysis",
  "main": "./dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "node scripts/dev.js",
    "dist": "vite build && electron-builder",
    "dist:mac": "vite build && electron-builder --mac",
    "dist:win": "vite build && electron-builder --win",
    "dist:linux": "vite build && electron-builder --linux"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@slack/web-api": "^7.9.2",
    "@supercharge/promise-pool": "^3.2.0",
    "app-root-path": "^3.1.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "color": "^5.0.0",
    "color-name": "^2.0.0",
    "detect-libc": "^2.0.4",
    "docx": "^9.5.0",
    "dotenv": "^16.5.0",
    "electron-store": "8",
    "emoji-datasource": "^15.1.2",
    "i18next": "^23.8.2",
    "lucide-react": "^0.320.0",
    "node-emoji": "^2.2.0",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "semver": "^7.7.2",
    "sharp": "^0.33.5",
    "slack-maxqda-adapter": "^2.2.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "universal-emoji-parser": "^2.0.52",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.naelab.slack-exporter",
    "productName": "Nae Slack to MAXQDA Adaptor",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "npmRebuild": true,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
